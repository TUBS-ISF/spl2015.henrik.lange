package learning_program;

import java.util.ArrayList;

public class Engine {

	public Engine() {
		// TODO Auto-generated constructor stub
	}

	boolean compareAllAnswer(ArrayList<String> answer, String givenAnswer) {
		if (answer.contains(givenAnswer)) {
			return true;
		} else {
			return false;
		}
	}

	boolean compareOneAnswer(ArrayList<String> answer, String givenAnswer) {
		if (answer.get(0).equals(givenAnswer)) {
			return true;
		} else {
			return false;
		}
	}

}
